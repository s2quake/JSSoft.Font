    "version": "0.2.0",
    "configurations": [
        {
            "name": "launch-console",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-console",
            "program": "${workspaceFolder}/exe/Debug/netcoreapp3.1/console/jsfont.dll",
            "args": [],
            "cwd": "${workspaceFolder}/exe/Debug/netcoreapp3.1/console",
            "stopAtEntry": false
        },
        {
            "name": "launch-application",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-application",
            "program": "${workspaceFolder}/exe/Debug/netcoreapp3.1/application/jsfontApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/exe/Debug/netcoreapp3.1/application",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}